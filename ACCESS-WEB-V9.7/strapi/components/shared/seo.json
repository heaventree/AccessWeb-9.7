{
  "collectionName": "components_shared_seos",
  "info": {
    "displayName": "SEO",
    "description": "Configure SEO metadata for pages"
  },
  "options": {},
  "attributes": {
    "metaTitle": {
      "type": "string",
      "required": true
    },
    "metaDescription": {
      "type": "text",
      "required": true
    },
    "keywords": {
      "type": "text"
    },
    "metaRobots": {
      "type": "string"
    },
    "structuredData": {
      "type": "json"
    },
    "metaViewport": {
      "type": "string"
    },
    "canonicalURL": {
      "type": "string"
    },
    "metaImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    }
  }
}